@@INCLUDE@@ set_babel.thtml

<h2>@_BABEL_LAST_USER_MESSAGES_@</h2>

<ul class="slides">
@@TABLE@@
  <li class="thumbs">
    <p><a href="@_FORUM_ENTRY_URL_@?TID=@_TID_@">
    @@IF@@ @_SIZE:NAME_@ > 20
    @_SLICE(1..20):NAME_@...
    @@ELSE@@
    @_NAME_@
    @@END_IF@@
    </a></p>
    <ul class="thumb_metadata">
      <li class="photocat">@_CATEGORY_@</li>
      <li>@_COMMENT_COUNTER_@ / @_VISIT_COUNTER_@</li>
    </ul>
  </li>
@@END_TABLE@@
</ul>
