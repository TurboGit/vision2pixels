@@INCLUDE@@ babel.thtml
@@-- HTTP_GET(FROM): index of the first post to output
<div id="forum_navigate">
@@IF@@ @_EXIST:PREVIOUS_@
  <a href="@_FORUM_THREAD_URL_@?FID=@_FID_@&FROM=@_PREVIOUS_@&CTX_ECWF=@_CTX_ECWF_@">@_BABEL_PREVIOUS_@</a>
@@END_IF@@
@@IF@@ @_EXIST:NEXT_@
  <a href="@_FORUM_THREAD_URL_@?FID=@_FID_@&FROM=@_NEXT_@&CTX_ECWF=@_CTX_ECWF_@">@_BABEL_NEXT_@</a>
@@END_IF@@
</div>
