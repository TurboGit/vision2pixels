<GPS>
  <Language>
    <Name>XML Template</Name>
    <Parent>XML</Parent>
    <Keywords mode="append">@@(TABLE|SET|E(LSE|ND_(IF|TABLE))|I(F|NCLUDE))@@|</Keywords>
    <Keywords>@_([^ ]*)_@</Keywords>
    <Context>
      <New_Line_Comment_Start mode="append">@@--</New_Line_Comment_Start>
      <String_Delimiter>&quot;</String_Delimiter>
      <Constant_Character>&apos;</Constant_Character>
      <Can_Indent>True</Can_Indent>
      <Syntax_Highlighting>True</Syntax_Highlighting>
      <Case_Sensitive>False</Case_Sensitive>
    </Context>
    <Spec_Suffix>.txml</Spec_Suffix>
  </Language>

